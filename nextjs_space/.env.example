
# Database
DATABASE_URL='postgresql://user:password@host:5432/database?connect_timeout=15'

# NextAuth
NEXTAUTH_SECRET=your-nextauth-secret-here-min-32-chars

# AWS Configuration (for file uploads)
AWS_PROFILE=hosted_storage
AWS_REGION=us-west-2
AWS_BUCKET_NAME=your-bucket-name
AWS_FOLDER_PREFIX=your-folder-prefix/

# Abacus AI API
ABACUSAI_API_KEY=your-abacusai-api-key-here

# Runway API (optional - for video generation)
RUNWAY_API_KEY=your-runway-api-key-here

# fal.ai API (for image/video generation)
FAL_API_KEY=your-fal-api-key-here
